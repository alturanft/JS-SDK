{
  "name": "@altura/altura-js",
  "version": "1.1.0",
  "description": "Javascript SDK for AlturaNFT",
  "license": "MIT",
  "author": "Altura Developers",
  "homepage": "https://www.alturanft.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alturanft/JS-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/alturanft/JS-SDK/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --coverage --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "keywords": [
    "altura",
    "nft",
    "marketplace",
    "node",
    "sdk",
    "smart-contracts",
    "javascript",
    "ethereum",
    "binance",
    "crypto"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "dotenv": "^16.0.1",
    "jest": "^28.1.0",
    "logging-library": "^0.5.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@walletconnect/web3-provider": "^1.7.8",
    "ethers": "^5.6.8",
    "isomorphic-unfetch": "^3.1.0",
    "query-string": "^7.1.1"
  }
}
